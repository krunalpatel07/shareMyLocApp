<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicbyrequest.sharemylocation" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Share My Location</name>
    <description>An awesome app where user can share location</description>
    <author email="krunalpatel07@hotmail.com" href="http://ionicframework.com/">Krunal Patel</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://ionic.local/*" />
    <allow-navigation href="http://10.37.45.166:8100" />
    <allow-navigation href="http://192.168.1.13:8100" />
    <allow-navigation href="http://10.37.45.46:8100" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/share-location.png" />
        <icon density="mdpi" src="resources/android/icon/share-location.png" />
        <icon density="hdpi" src="resources/android/icon/share-location.png" />
        <icon density="xhdpi" src="resources/android/icon/share-location.png" />
        <icon density="xxhdpi" src="resources/android/icon/share-location.png" />
        <icon density="xxxhdpi" src="resources/android/icon/share-location.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="resources/ios/icon/share-location.png" width="57" />
        <icon height="114" src="resources/ios/icon/share-location.png" width="114" />
        <icon height="40" src="resources/ios/icon/share-location.png" width="40" />
        <icon height="80" src="resources/ios/icon/share-location.png" width="80" />
        <icon height="120" src="resources/ios/icon/share-location.png" width="120" />
        <icon height="50" src="resources/ios/icon/share-location.png" width="50" />
        <icon height="100" src="resources/ios/icon/share-location.png" width="100" />
        <icon height="60" src="resources/ios/icon/share-location.png" width="60" />
        <icon height="120" src="resources/ios/icon/share-location.png" width="120" />
        <icon height="180" src="resources/ios/icon/share-location.png" width="180" />
        <icon height="72" src="resources/ios/icon/share-location.png" width="72" />
        <icon height="144" src="resources/ios/icon/share-location.png" width="144" />
        <icon height="76" src="resources/ios/icon/share-location.png" width="76" />
        <icon height="152" src="resources/ios/icon/share-location.png" width="152" />
        <icon height="167" src="resources/ios/icon/share-location.png" width="167" />
        <icon height="29" src="resources/ios/icon/share-location.png" width="29" />
        <icon height="58" src="resources/ios/icon/share-location.png" width="58" />
        <icon height="87" src="resources/ios/icon/share-location.png" width="87" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-console" spec="^1.0.5" />
    <plugin name="cordova-plugin-device" spec="^1.1.4" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3" />
    <plugin name="cordova-plugin-nativegeocoder" spec="^1.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.1" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.1.8" />
    <plugin name="cordova-plugin-x-toast" spec="^2.6.0" />
    <plugin name="ionic-plugin-keyboard" spec="^2.2.1" />
</widget>
